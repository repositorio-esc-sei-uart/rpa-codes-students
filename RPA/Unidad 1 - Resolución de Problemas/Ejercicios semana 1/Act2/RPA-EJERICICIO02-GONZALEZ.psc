Algoritmo determinar_suma_resta_division_multiplicacion
	Definir suma_resta_division_multiplicacion Como Entera
	
	Definir B Como Entera
	
	Definir A Como Entera
	Leer A
	Leer B
	
	SUMA <- A + B
	Escribir SUMA
	
	RESTA <- A - B
	Escribir RESTA
	
	DIVISION <- A / B
	Escribir DIVISION
	
	MULTIPLICACION <- A * B
	Escribir MULTIPLICACION
	
	FinAlgoritmo
