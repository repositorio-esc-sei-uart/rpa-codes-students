Algoritmo CambioValor
	Definir a,b,c Como Entero;
	Escribir "Escribir el valor de a";
	Leer a;
	Escribir "Escribir el valor de b";
	Leer b;
	Escribir "Escribir el valor de c";
	Leer c;
	Mostrar "El valor inicial de a es: ",a;
	Mostrar "El valor inicial de b es: ",b;
	Mostrar "El valor inicial de c es: ",c;
	a<-b
	b<-c
	c<-a
	Mostrar "El valor final de A es: ",a;
	Mostrar "El valor final de B es: ",b;
	Mostrar "El valor final de c es: ",c;
FinAlgoritmo
