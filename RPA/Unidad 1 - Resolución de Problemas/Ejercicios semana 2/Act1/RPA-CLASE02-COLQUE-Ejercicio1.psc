Proceso Intercambio_de_variables
	
	Definir A Como Entero
	Definir B Como Entero
	Definir C Como Entero
	Definir aux Como Entero
	
	// variables
	A<-2
	B<-10
	C<-40
	
	// Intercambio de Variables B y A
	aux<-B
	B<-A
	A<-aux
	
	// Intercambio de Variables C y B
	aux<-C
	C<-B
	B<-aux
	
	// Intercambio de variables A y C
	aux<-A
	A<-C
	C<-aux
	
FinProceso
