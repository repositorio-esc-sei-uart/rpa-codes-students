Algoritmo intercambios_variables 
	// declaraciones 
	Definir A Como Entero
	Definir B Como Entero
	Definir C Como Entero
	Definir aux  Como Entero
	
	// inicializacion 
	A =  2
	B= 10 
	C = 40 
	aux= A
	
	//Proceso 
	aux  <- A;
	A<-B;
	B<-C;
	C<-aux;
	
	
	
	Escribir "Intercambio:" "A="  A; 
	Escribir "Intercambio :" "B=" B; 
	Escribir "Intercambio: " "C=" C;
	
	
FinAlgoritmo
